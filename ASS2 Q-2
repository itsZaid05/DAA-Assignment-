#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int n = 5;
    int at[] = {900, 910, 920, 1100, 1120};
    int dt[] = {940, 1200, 950, 1130, 1140};
    sort(at, at + n);
    sort(dt, dt + n);
    int i = 0, j = 0;
    int platforms = 0, maxPlatforms = 0;
    while (i < n && j < n) {
        if (at[i] <= dt[j]) {
            platforms++;
            maxPlatforms = max(maxPlatforms, platforms);
            i++;
        } else {
            platforms--;
            j++;
        }
    }

    cout << "Minimum number of platforms required = " << maxPlatforms;
    return 0;
}
